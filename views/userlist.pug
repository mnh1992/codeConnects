extend layout
block content
    h1.
        Teacher List
    ul
        each user in userlist
            div.well.container
                p
                    strong Name
                    |: #{user.teacher.name}
                    br
                    
                    strong Email:
                    a(href="mailto:#{user.email}")= user.email
                    br
                    
                    strong Approval Status
                    |: #{user.approved}
            